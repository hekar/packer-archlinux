{
    "archinstall-language": "English",
    "sys-language": "en_US",
    "sys-encoding": "UTF-8",
    "keyboard-layout": "us",
    "swap": true,
    "hostname": "archlinux",
    "bootloader": "grub-install",
    "audio": "pipewire",
    "timezone": "UTC",
    "ntp": true,
    "parallel downloads": true,
    "config_version": "2.5.1",
    "debug": false,
    "harddrives": [
        "/dev/vda"
    ],
    "kernels": [
        "linux"
    ],
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "offline": false,
    "packages": [
        "git",
        "openssh",
        "docker",
        "docker-compose",
        "vim",
        "zsh"
    ],
    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/server.py"
    },
    "servers": [
        "docker",
        "sshd"
    ],
    "script": "guided",
    "silent": true,
    "version": "2.5.1"
}